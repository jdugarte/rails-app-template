<% unless users.empty? %>
  <%= link_to("", "#", {:name => "buffs"}) %>
  <h3 class="title"><%= title %></h3>
  <ul class="list avatar">
    <% users.each do |user| %>
      <li>
        <div class="left">
          <%= link_to image_tag(user.avatar.url(:thumb)), profile_path(user.username) %>
        </div>
        <div class="item">
          <%= link_to_user_profile user %>
        </div>
      </li>
    <% end %>
  </ul>
<% end %>

